pipeline {
        agent any
        stages {
                stage ('SCM checkout') {
                                        steps {
                                              git "https://github.com/yogeshgund/maven-project.git"
                                              }
                                       }
                 stage ('install')     {
                                  steps {
                                         withMaven(maven: 'localmaven') {
                                                                         sh 'mvn clean install'
                                                                        }
                                        }
                                       }
               }
        }
