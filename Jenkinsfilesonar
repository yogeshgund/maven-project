pipeline {
          agent any {
                     stages {	
	                           stage "SCM Checkout" {
	                                                 git 'https://github.com/yogeshgund/maven-project.git'
	                                                }
                            }
                            {
                             stage ('Testing Stage') {
                                                      steps {
                                                             withMaven(maven : 'LocalMaven')
                                                                                            {
                                                                                             sh 'mvn test'
                                                                                            }
                                                             }
                                                      } 
                            }
                     }
          }
