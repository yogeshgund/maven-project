    
node{
    stage ('scm checkout') {
	git ('https://github.com/prakashk0301/maven-project')
	}
	
	stage('Checkout to different branch') {
           sh "git branch -r"
           sh "git checkout try-docker"
    }
        stage ('package stage') {
           sh label: '', script: 'mvn clean package '
        }
 }
 
 
